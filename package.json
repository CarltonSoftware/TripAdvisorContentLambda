{
  "name": "tripadvisorcontentlambda",
  "version": "1.0.0",
  "description": "Lambda function for updating availability on trip advisor",
  "main": "index.js",
  "scripts": {
    "updatePropertyAvailability": "run-func scripts.js updatePropertyAvailability $PROPERTY_ID",
    "updateAllPropertyAvailability": "run-func scripts.js updateAllPropertyAvailability",
    "updateYesterdaysBookings": "run-func scripts.js updateYesterdaysBookings",
    "DISCO": "run-func scripts.js testDISCO",
    "postinstall": "npm link plato-js-client",
    "zip": "rm -f module.zip && zip module.zip -r node_modules scripts.js handler.js"
  },
